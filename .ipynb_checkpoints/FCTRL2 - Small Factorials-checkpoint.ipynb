    "#http://www.spoj.com/problems/FCTRL2/\n",
    "\n",
    "def factorial(n):\n",
    "    if (n == 1):\n",
    "        return (1)\n",
    "    else:\n",
    "        return(n*factorial(n-1))\n",
    "\n",
    "cases = input()\n",
    "for i in range(cases):\n",
    "    n = input()\n",
    "    print(factorial(n))\n",
